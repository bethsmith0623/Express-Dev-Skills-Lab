<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <meta http-equiv="X-UA-Compatible" content="ie=edge">
  <link rel='stylesheet' href='/stylesheets/style.css' />
  <title>Dev Skills</title>
</head>
<body>
  <h2>My Growing Developer Skills</h2>
  <table>
    <thead>
      <tr>
        <th>Skill</th>
        <th>Current Level</th>
        <th>More Details</th>
      </tr>
    </thead>
    <tbody>
      <% skills.forEach(function(s) { %>
        <tr>
          <td id="skill"><%= s.skill %></td>
          <td id="level"><%= s.level %></td>
          <td><input type="submit" id="details"></td>
          
      </tr>
      <% }) %>
    </tbody>
  </table>
</body>
</html>